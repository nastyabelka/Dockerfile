{
    "family": "Mylast-GW-CI-task",
    "taskRoleArn": "",
    "executionRoleArn": "",
    "networkMode": "bridge",
    "containerDefinitions": [
        {
            "name": "Mylast-GW-CI-task",
            "image": "192608593085.dkr.ecr.us-east-1.amazonaws.com/gw:NNNNN",
            "cpu": 256,
            "memory": 512,
            "portMappings": [
                {
                    "hostPort": 80,
                    "containerPort": 80,
                    "protocol": "tcp"
                }
            ],
            "essential": true,
            "startTimeout": 2,
            "stopTimeout": 2,
            "ulimits": [
                {
                    "name": "msgqueue",
                    "softLimit": 0,
                    "hardLimit": 0
                }
            ]
        }
    ],
    "requiresCompatibilities": [
        "EC2"
    ],
    "cpu": "256",
    "memory": "512",
    "pidMode": "task",
    "ipcMode": "none"
}
